{
    "name": "Fake ID Sharing",
    "description": "Sharing Fake IDs to protect your privacy.",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_icon": "/img/logo128.png",
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "/img/logo16.png",
        "32": "/img/logo32.png",
        "48": "/img/logo48.png",
        "128": "/img/logo128.png"
    },
    "permissions": ["tabs", "storage"],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
